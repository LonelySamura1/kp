<Window x:Class="KP.Forms.MasterForms.CloseOrderForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KP.Forms.MasterForms"
        mc:Ignorable="d"
        Title="закрыть заказ" Height="136.25" Width="181.25" Loaded="Window_Loaded">
    <Grid>
        <StackPanel>
            <WrapPanel>
                <Label Content="Цена: "/>
                <TextBox x:Name="Pricetxt" Width="100"/>
            </WrapPanel>
            <Button Content="Закрыть" Margin="0,55,0,0" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
